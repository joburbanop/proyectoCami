{
  "name": "pagina-web",
  "version": "1.0.0",
  "description": "proyecto portafolio de maquillaje",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "dev": "lite-server",
    "preminify:dist": "mkdir -p dist",
    "clean": "rimraf dist",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "minify:img": "imagemin imagenesUtilizadas/* -o dist/imagenesUtilizadas",
    "minify:js": "npm run preminify:dist && terser javaScript/*.js -o dist/main.min.js --compress --mangle",
    "minify:css": "usemin index.html -d  dist --htmlin -o dist/index.html && usemin HTML/Cursos.html -d  dist --htmlin -o dist/Cursos.html",
    "minify": "npm run minify:js && npm run minify:css",
    "build": "npm run clean && npm run scss && npm run minify:img && npm run minify && npm run minify-html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joburbanop/proyectoCami.git"
  },
  "author": "joburbanop",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joburbanop/proyectoCami/issues"
  },
  "homepage": "https://github.com/joburbanop/proyectoCami#readme",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "cssnano": "^5.1.15",
    "html-minifier": "^4.0.0",
    "htmlmin": "^0.0.7",
    "imagemin": "^8.0.1",
    "lite-server": "^2.6.1",
    "node-sass": "^8.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^4.1.2",
    "terser": "^5.16.5",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "bootstrap": "^5.2.3",
    "jquery": "^3.6.3",
    "popper.js": "^1.16.1"
  }
}
